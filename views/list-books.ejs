<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List Books</title>
</head>
<body>
    <h1>List Books</h1>
    <form action="/books" method="get">
        <input type="text" name="search" id="">
        <button type="submit">Search</button>
    </form>
    <table>
        <thead>
            <tr>
                <td>Image</td>
                <td>Title</td>
                <td>Author</td>
                <td>Publisher</td>
                <td>Link</td>
            </tr>
        </thead>
        <tbody>
            <% result.forEach(el => { %>
                <tr>
                    <td>
                        <img style="max-width: 200px;" src="<%=el.imageUrl%>" alt="">
                    </td>
                    <td><%= el.title%></td>
                    <td><%= el.author%></td>
                    <td><%= el.Publisher.name%></td>
                    <td><%= el.link%></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>